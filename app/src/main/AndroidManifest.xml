<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.cysion.ankosample">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.CommonActivity">
        </activity>
        <activity android:name=".activity.LayoutActvity">
        </activity>
        <activity android:name=".activity.CorountineActivity">
        </activity>
        <activity android:name=".activity.SQliteActivity">
        </activity>
        <activity android:name=".activity.IntentActivity">
        </activity>
    </application>

</manifest>